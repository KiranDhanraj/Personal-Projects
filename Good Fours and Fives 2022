// Online C++ compiler to run C++ program online
#include <iostream>

using namespace std;
int ans=0;
int is_divisible( int x){
    
    for( int i=0; i<(x/5)+1; i++){
        if ((x-(i*5))%4==0){
            ans++;
            
        }
        
        
        
    }
    return ans;    
    
    
    
    
}
int main() {
    int num;
    cin>>num;
    cout<<is_divisible(num);

    return 0;
}
